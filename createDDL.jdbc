CREATE TABLE CUSTOMERORDER_WIDGET (CustomerOrder_ID BIGINT NOT NULL, widgets_ID BIGINT NOT NULL, PRIMARY KEY (CustomerOrder_ID, widgets_ID))
ALTER TABLE CUSTOMERORDER_WIDGET ADD CONSTRAINT CSTMRRDRWDGTwdgtsD FOREIGN KEY (widgets_ID) REFERENCES WIDGET (ID)
